    <Method Name="resetAlarms" Id="{dee7393a-14d0-406b-8af1-5eb7c794c2f8}">
      <Declaration><![CDATA[METHOD resetAlarms
VAR_INPUT
END_VAR
VAR
	i:INT;
	ToolData: INT;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[FOR i:=1 TO 20 DO
	Alarms[i] := FALSE;
END_FOR
]]></ST>
